{
  "name": "sveltekit-graphql-github",
  "version": "0.1.0",
  "scripts": {
    "dev": "svelte-kit dev -p 3000",
    "build": "sh ./patch-apollo-client.sh && svelte-kit build",
    "preview": "svelte-kit preview -p 3000",
    "check": "svelte-check --fail-on-warnings",
    "check:watch": "svelte-check --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "lint:scss": "stylelint \"src/**/*.{css,scss,svelte}\"",
    "format": "prettier --write --plugin-search-dir=. .",
    "prettier:check": "prettier --check --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@apollo/client": "3.5.6",
    "@commitlint/config-conventional": "^16.2.1",
    "@fontsource/fira-sans": "^4.5.3",
    "@fontsource/lato": "^4.5.2",
    "@fontsource/merriweather": "^4.5.5",
    "@sveltejs/adapter-static": "^1.0.0-next.28",
    "@sveltejs/kit": "next",
    "dayjs": "^1.10.8",
    "dotenv": "^16.0.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "graphql": "^15.8.0",
    "graphql-tag": "^2.12.6",
    "husky": "^7.0.4",
    "node-fetch": "^2.6.7",
    "postcss": "^8.4.7",
    "postcss-html": "^1.3.0",
    "prettier": "~2.4.1",
    "prettier-plugin-svelte": "^2.6.0",
    "sass": "^1.49.9",
    "stylelint": "^14.5.3",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-scss": "^4.1.0",
    "svelte": "^3.46.4",
    "svelte-check": "^2.4.5",
    "svelte-feather-icons": "^3.5.1",
    "svelte-preprocess": "^4.10.4"
  },
  "type": "module"
}
